#!/bin/bash

set -euo pipefail

#####################################################
# This algorithm is implemented in the scikit-learn
# software package (http://scikit-learn.org/stable/).
#####################################################

dataFilePath="$1"

python AlgorithmScripts/Helper/sklearn_f_generic.py "$dataFilePath" "score" "from sklearn.feature_selection import mutual_info_classif; score = 1 - mutual_info_classif(train_X, train_y)" 2>&1 | python AlgorithmScripts/Helper/ReformatSklearnOutput.py
