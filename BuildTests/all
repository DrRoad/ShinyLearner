#!/bin/bash

set -o errexit

inputDir=/input
outputDir=/output
classifAlgo="/AlgorithmScripts/Classification/tsv/sklearn/*/default"
debug=false

for description in StrongSignal_Both
do
  dataFile=$inputDir/${description}.tsv.gz

  rm -fv output/*

  /BuildTests/nestedclassification_montecarlo $dataFile $description "$classifAlgo" $debug $outputDir
  /BuildTests/check $description "$classifAlgo"

  rm -v output/*
done
