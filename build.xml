<project default="jar">
  <target name="jar">
    <jar destfile="shinylearner.jar"
        basedir="shinylearner/src"
        includes="**/*.class">
      <manifest>
        <attribute name="Main-Class" value="shinylearner.Main" />
        <attribute name="Class-Path" value="lib/paldb.jar" />
      </manifest>
    </jar>
  </target>
</project>

